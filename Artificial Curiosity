  .md\:pr-\[138px\] { padding-right: 138px; }
  .md\:pr-\[59px\] { padding-right: 59px; }
  .md\:pt-md { padding-top: 16px; }
  .md\:pt-xl { padding-top: 48px; }
  .md\:text-left { text-align: left; }
  .md\:text-6xl { font-size: 3.75rem; line-height: 1; }
  .md\:text-\[22px\] { font-size: 22px; }
  .md\:text-\[28px\] { font-size: 28px; }
  .md\:hover\:bg-offsetPlus:hover { --tw-bg-opacity:1; background-color: rgb(233 233 234/var(--tw-bg-opacity)); }
  .md\:hover\:text-super:hover { --tw-text-opacity:1; color: rgb(0 122 255/var(--tw-text-opacity)); }
  .md\:hover\:text-superAlt:hover { --tw-text-opacity:1; color: rgb(167 58 58/var(--tw-text-opacity)); }
  .md\:hover\:text-textMain:hover { --tw-text-opacity:1; color: rgb(39 39 42/var(--tw-text-opacity)); }
  .group:hover .md\:group-hover\:fill-super { fill: rgb(0, 122, 255); }
  .group:hover .md\:group-hover\:fill-white { fill: rgb(255, 255, 255); }
  .group:hover .md\:group-hover\:text-super { --tw-text-opacity:1; color: rgb(0 122 255/var(--tw-text-opacity)); }
  .dark .dark\:md\:bg-offsetDark { --tw-bg-opacity:1; background-color: rgb(32 32 37/var(--tw-bg-opacity)); }
  .dark .dark\:md\:hover\:bg-offsetPlusDark:hover, .dark .md\:dark\:hover\:bg-offsetPlusDark:hover, .dark .md\:hover\:dark\:bg-offsetPlusDark:hover { --tw-bg-opacity:1; background-color: rgb(42 42 51/var(--tw-bg-opacity)); }
  .dark .dark\:md\:hover\:text-textMainDark:hover { --tw-text-opacity:1; color: rgb(206 206 210/var(--tw-text-opacity)); }
}

