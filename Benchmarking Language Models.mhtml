(define addo
  (lambda (a b c)
    (conde
      [(== a 0) (== b c)]                  ; base case: a is 0, b = c
      [(fresh (a')                          ; recursive case: decompose a
               (== a `(,a' . ,a))           ; a' is the predecessor of a
               (addo a' b c'))])))         ; recursively add a' and b, c'

;;; Prove a theorem: for any natural number a, adding 0 to a yields a
(run* (a)
  (addo a 0 a))
