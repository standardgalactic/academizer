core.data.materials.append(mat)

bpy.ops.mesh.primitive_plane_add(size=0.15)
fin = bpy.context.object
fin.scale[0], fin.scale[1] = 0.8, 0.28
fin.location[1] = 0.08
fin.rotation_euler[0] = math.radians(15)
