width="8" height="4" rx="1" ry="1"></rect></svg>Copy code</button></div><div class="p-4 overflow-y-auto"><code class="!whitespace-pre hljs language-python"><span class="hljs-keyword">import</span> random
<span class="hljs-keyword">import</span> string

<span class="hljs-comment"># Functions for the agents</span>
<span class="hljs-keyword">def</span> <span class="hljs-title function_">insert_error</span>(<span class="hljs-params">sentence</span>):
    idx = random.randint(<span class="hljs-number">0</span>, <span class="hljs-built_in">len</span>(sentence) - <span class="hljs-number">1</span>)
    sentence[idx] = random.choice(string.ascii_lowercase)
    <span class="hljs-keyword">return</span> sentence

<span class="hljs-keyword">def</span> <span class="hljs-title function_">detect_error</span>(<span class="hljs-params">original, modified</span>):
    <span class="hljs-keyword">for</span> i <span class="hljs-keyword">in</span> <span class="hljs-built_in">range</span>(<span class="hljs-built_in">len</span>(original)):
        <span class="hljs-keyword">if</span> original[i] != modified[i]:
            <span class="hljs-keyword">return</span> i
    <span class="hljs-keyword">return</span> -<span class="hljs-number">1</span>

<span class="hljs-comment"># Crossover function</span>
<span class="hljs-keyword">def</span> <span class="hljs-title function_">crossover</span>(<span class="hljs-params">parent1, parent2</span>):
    <span class="hljs-keyword">return</span> [(fn1 <span class="hljs-keyword">if</span> random.random() &lt; <span class="hljs-number">0.5</span> <span class="hljs-keyword">else</span> fn2) <span class="hljs-keyword">for</span> fn1, fn2 <span class="hljs-keyword">in</span> <span class="hljs-built_in">zip</span>(parent1, parent2)]

<span class="hljs-comment"># Mutation function</span>
<span class="hljs-keyword">def</span> <span class="hljs-title function_">mutate</span>(<span class="hljs-params">agent</span>):
    <span class="hljs-keyword">if</span> random.random() &lt; <span class="hljs-number">0.1</span>:
        <span class="hljs-keyword">return</span> insert_error <span class="hljs-keyword">if</span> agent == detect_error <span class="hljs-keyword">else</span> detect_error
    <span class="hljs-keyword">return</span> agent

<span class="hljs-comment"># Initialize populations</span>
inserters = [insert_error <span class="hljs-keyword">for</span> _ <span class="hljs-keyword">in</span> <span class="hljs-built_in">range</span>(<span class="hljs-number">10</span>)]
detectors = [detect_error <span class="hljs-keyword">for</span> _ <span class="hljs-keyword">in</span> <span class="hljs-built_in">range</span>(<span class="hljs-number">10</span>)]

<span class="hljs-comment"># Evolution loop</span>
<span class="hljs-keyword">for</span> generation <span class="hljs-keyword">in</span> <span class="hljs-built_in">range</span>(<span class="hljs-number">1</span>, <span class="hljs-number">6</span>):
    inserter_scores = [<span class="hljs-number">0</span>]*<span class="hljs-number">10</span>
    detector_scores = [<span class="hljs-number">0</span>]*<span class="hljs-number">10</span>
    
    <span class="hljs-comment"># Simulation for scoring</span>
    <span class="hljs-keyword">for</span> _ <span class="hljs-keyword">in</span> <span class="hljs-built_in">range</span>(<span class="hljs-number">100</span>):
        sentence = [random.choice(string.ascii_lowercase) <span class="hljs-keyword">for</span> _ <span class="hljs-keyword">in</span> <span class="hljs-built_in">range</span>(<span class="hljs-number">10</span>)]
        original_sentence = sentence.copy()
        
        inserter = random.choice(inserters)
        detector = random.choice(detectors)
        
        modified_sentence = inserter(sentence.copy())
        detected_idx = detector(original_sentence, modified_sentence)
        
        actual_idx = detect_error(original_sentence, modified_sentence)
        
        <span class="hljs-keyword">if</span> detected_idx == actual_idx:
            detector_scores[detectors.index(detector)] += <span class="hljs-number">100</span>
            inserter_scores[inserters.index(inserter)] -= <span class="hljs-number">100</span>
        <span class="hljs-keyword">else</span>:
            detector_scores[detectors.index(detector)] += <span class="hljs-number">0</span>
            inserter_scores[inserters.index(inserter)] += <span class="hljs-number">0</span>

    <span class="hljs-comment"># Selection, Crossover, Mutation</span>
    best_inserters = <span class="hljs-built_in">sorted</span>(<span class="hljs-built_in">range</span>(<span class="hljs-built_in">len</span>(inserters)), key=<span class="hljs-keyword">lambda</span> k: inserter_scores[k])[:<span class="hljs-number">5</span>]
    best_detectors = <span class="hljs-built_in">sorted</span>(<span class="hljs-built_in">range</span>(<span class="hljs-built_in">len</span>(detectors)), key=<span class="hljs-keyword">lambda</span> k: detector_scores[k], reverse=<span class="hljs-literal">True</span>)[:<span class="hljs-number">5</span>]
    
    new_inserters = [inserters[i] <span class="hljs-keyword">for</span> i <span class="hljs-keyword">in</span> best_inserters]
    new_detectors = [detectors[i] <span class="hljs-keyword">for</span> i <span class="hljs-keyword">in</span> best_detectors]
    
    <span class="hljs-keyword">while</span> <span class="hljs-built_in">len</span>(new_inserters) &lt; <span class="hljs-number">10</span>:
        parent1, parent2 = random.choices(best_inserters, k=<span class="hljs-number">2</span>)
        child = crossover([inserters[parent1]], [inserters[parent2]])[<span class="hljs-number">0</span>]
        child = mutate(child)
        new_inserters.append(child)
        
    <span class="hljs-keyword">while</span> <span class="hljs-built_in">len</span>(new_detectors) &lt; <span class="hljs-number">10</span>:
        parent1, parent2 = random.choices(best_detectors, k=<span class="hljs-number">2</span>)
        child = crossover([detectors[parent1]], [detectors[parent2]])[<span class="hljs-number">0</span>]
        child = mutate(child)
        new_detectors.append(child)
    
    inserters = new_inserters
    detectors = new_detectors
    
    <span class="hljs-built_in">print</span>(<span class="hljs-string">f"Generation <span class="hljs-subst">{generation}</span>"</span>)
    <span class="hljs-built_in">print</span>(<span class="hljs-string">f"Inserter Scores: <span class="hljs-subst">{inserter_scores}</span>"</span>)
