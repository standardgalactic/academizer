<span class="hljs-keyword">import</span> numpy <span class="hljs-keyword">as</span> np

<span class="hljs-comment"># Define a sample fuzzy membership function (triangular)</span>
<span class="hljs-keyword">def</span> <span class="hljs-title function_">triangular_mf</span>(<span class="hljs-params">x, a, b, c</span>):
    <span class="hljs-keyword">if</span> x &lt;= a <span class="hljs-keyword">or</span> x &gt;= c:
        <span class="hljs-keyword">return</span> <span class="hljs-number">0</span>
    <span class="hljs-keyword">elif</span> a &lt; x &lt;= b:
        <span class="hljs-keyword">return</span> (x - a) / (b - a)
    <span class="hljs-keyword">else</span>:
        <span class="hljs-keyword">return</span> (c - x) / (c - b)

<span class="hljs-comment"># Define a sample fuzzy inference system with two rules</span>
<span class="hljs-keyword">def</span> <span class="hljs-title function_">fuzzy_inference</span>(<span class="hljs-params">input_value</span>):
    <span class="hljs-comment"># Define fuzzy rules</span>
    rule1 = <span class="hljs-built_in">min</span>(triangular_mf(input_value, <span class="hljs-number">1</span>, <span class="hljs-number">2</span>, <span class="hljs-number">3</span>), triangular_mf(input_value, <span class="hljs-number">4</span>, <span class="hljs-number">5</span>, <span class="hljs-number">6</span>))
    rule2 = <span class="hljs-built_in">min</span>(triangular_mf(input_value, <span class="hljs-number">7</span>, <span class="hljs-number">8</span>, <span class="hljs-number">9</span>), triangular_mf(input_value, <span class="hljs-number">2</span>, <span class="hljs-number">3</span>, <span class="hljs-number">4</span>))
    
    <span class="hljs-comment"># Aggregate the rules (maximum operator)</span>
    aggregated_output = <span class="hljs-built_in">max</span>(rule1, rule2)
    
    <span class="hljs-comment"># Handle division by zero gracefully</span>
    <span class="hljs-keyword">if</span> rule1 == <span class="hljs-number">0</span> <span class="hljs-keyword">and</span> rule2 == <span class="hljs-number">0</span>:
        defuzzified_output = <span class="hljs-number">0</span>  <span class="hljs-comment"># Avoid division by zero</span>
    <span class="hljs-keyword">else</span>:
        <span class="hljs-comment"># Calculate centroids</span>
        centroids = [<span class="hljs-number">2.5</span>, <span class="hljs-number">5.5</span>]  <span class="hljs-comment"># Centers of gravity for the two rules</span>
        <span class="hljs-comment"># Calculate the weighted sum of centroids and the sum of rule strengths</span>
        weighted_centroids = np.multiply(centroids, [rule1, rule2])
        sum_rule_strengths = rule1 + rule2
        <span class="hljs-comment"># Calculate the defuzzified output</span>
        defuzzified_output = <span class="hljs-built_in">sum</span>(weighted_centroids) / sum_rule_strengths
    
    <span class="hljs-keyword">return</span> defuzzified_output

<span class="hljs-comment"># Input value</span>
input_value = <span class="hljs-number">3.5</span>

<span class="hljs-comment"># Calculate the defuzzified output</span>
