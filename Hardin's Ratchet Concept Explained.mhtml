    </span><span><span class="hljs-keyword">for</span></span><span> cycle </span><span><span class="hljs-keyword">in</span></span><span> </span><span><span class="hljs-built_in">range</span></span><span>(</span><span><span class="hljs-number">1</span></span><span>, exposure_cycles):
