$Env:SYMBOLIC_ENGINE_API_KEY="&lt;WOLFRAMALPHA_API_KEY&gt;"
$Env:SEARCH_ENGINE_API_KEY="&lt;SERP_API_KEY&gt;"
$Env:OCR_ENGINE_API_KEY="&lt;APILAYER_API_KEY&gt;"
