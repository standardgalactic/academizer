;; because apparently ^s and ^q start and stop
;; the terminal stream

!q::
	Random, OutputVar, 1, 100000
 	rn := RandomName(4,0) ;"-" OutputVar
	Sleep, 500
	Clipboard = %rn%
	Sleep, 500
	MouseClick, right
