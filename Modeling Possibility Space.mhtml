<span class="hljs-keyword">def</span> <span class="hljs-title function_">my_gamma</span>(<span class="hljs-params">x</span>):
    <span class="hljs-comment"># Coefficients for the Lanczos approximation</span>
    coefficients = [
        <span class="hljs-number">76.18009173</span>, -<span class="hljs-number">86.50532033</span>, <span class="hljs-number">24.01409822</span>, -<span class="hljs-number">1.231739516</span>,
        <span class="hljs-number">0.00120858003</span>, -<span class="hljs-number">0.00000536382</span>
    ]
    
    <span class="hljs-keyword">if</span> x &lt;= <span class="hljs-number">0</span>:
        <span class="hljs-keyword">raise</span> ValueError(<span class="hljs-string">"x must be a positive number"</span>)
    
    y = x
    tmp = x + <span class="hljs-number">5.5</span>
    tmp -= (x + <span class="hljs-number">0.5</span>) * math.log(tmp)
    
    ser = <span class="hljs-number">1.000000000190015</span>
    <span class="hljs-keyword">for</span> coefficient <span class="hljs-keyword">in</span> coefficients:
        y += <span class="hljs-number">1</span>
        ser += coefficient / y
    
    <span class="hljs-keyword">return</span> <span class="hljs-number">1.0000000000000005</span> * (<span class="hljs-number">2.5066282746310005</span> * ser / x) * math.exp(-tmp)

<span class="hljs-keyword">def</span> <span class="hljs-title function_">n_dimensional_sphere_volume</span>(<span class="hljs-params">n, radius</span>):
    <span class="hljs-keyword">if</span> n &lt;= <span class="hljs-number">0</span>:
        <span class="hljs-keyword">raise</span> ValueError(<span class="hljs-string">"n must be a positive integer"</span>)
    
    <span class="hljs-keyword">if</span> radius &lt;= <span class="hljs-number">0</span>:
        <span class="hljs-keyword">raise</span> ValueError(<span class="hljs-string">"Radius must be a positive number"</span>)
    
    volume = (math.pi**(n/<span class="hljs-number">2</span>) * radius**n) / my_gamma(n/<span class="hljs-number">2</span> + <span class="hljs-number">1</span>)
    <span class="hljs-keyword">return</span> volume

<span class="hljs-comment"># Example usage</span>
