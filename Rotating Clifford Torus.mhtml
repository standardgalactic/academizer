4 24" stroke-linecap="round" stroke-linejoin="round" class="h-4 w-4" height="1em" width="1em" xmlns="http://www.w3.org/2000/svg"><path d="M16 4h2a2 2 0 0 1 2 2v14a2 2 0 0 1-2 2H6a2 2 0 0 1-2-2V6a2 2 0 0 1 2-2h2"></path><rect x="8" y="2" width="8" height="4" rx="1" ry="1"></rect></svg>Copy code</button></div><div class="p-4 overflow-y-auto"><code class="!whitespace-pre hljs language-python"><span class="hljs-keyword">import</span> bpy
<span class="hljs-keyword">import</span> numpy <span class="hljs-keyword">as</span> np
<span class="hljs-keyword">import</span> math

<span class="hljs-comment"># Function to create a rotating sphere with a red sphere inside</span>
<span class="hljs-keyword">def</span> <span class="hljs-title function_">rotating_sphere</span>(<span class="hljs-params">theta, phi, rho</span>):
    x = rho * np.sin(theta) * np.cos(phi)
    y = rho * np.sin(theta) * np.sin(phi)
    z = rho * np.cos(theta)
    <span class="hljs-keyword">return</span> x, y, z

<span class="hljs-comment"># Custom colormap from green to amber to red</span>
<span class="hljs-keyword">def</span> <span class="hljs-title function_">green_to_amber_to_red_cmap</span>():
    colors = [(<span class="hljs-number">0</span>, (<span class="hljs-number">0</span>, <span class="hljs-number">1</span>, <span class="hljs-number">0</span>)), (<span class="hljs-number">0.7</span>, (<span class="hljs-number">1</span>, <span class="hljs-number">1</span>, <span class="hljs-number">0</span>)), (<span class="hljs-number">0.8</span>, (<span class="hljs-number">1</span>, <span class="hljs-number">0.5</span>, <span class="hljs-number">0</span>)), (<span class="hljs-number">1</span>, (<span class="hljs-number">1</span>, <span class="hljs-number">0</span>, <span class="hljs-number">0</span>))]
    <span class="hljs-keyword">return</span> colors

<span class="hljs-comment"># Function to create the rotating sphere object</span>
<span class="hljs-keyword">def</span> <span class="hljs-title function_">create_rotating_sphere</span>():
    <span class="hljs-comment"># Create a mesh for the rotating sphere</span>
    theta, phi = np.mgrid[<span class="hljs-number">0</span>:math.pi:<span class="hljs-number">100j</span>, <span class="hljs-number">0</span>:<span class="hljs-number">2</span> * math.pi:<span class="hljs-number">100j</span>]
    rho = <span class="hljs-number">2.0</span>
    x, y, z = rotating_sphere(theta, phi, rho)

    <span class="hljs-comment"># Create the mesh object</span>
    mesh = bpy.data.meshes.new(<span class="hljs-string">'SphereMesh'</span>)
    mesh.from_pydata(<span class="hljs-built_in">list</span>(<span class="hljs-built_in">zip</span>(x.ravel(), y.ravel(), z.ravel())), [], [])
    mesh.update()

    <span class="hljs-keyword">return</span> mesh

<span class="hljs-comment"># Function to create the material for the rotating sphere</span>
<span class="hljs-keyword">def</span> <span class="hljs-title function_">create_sphere_material</span>():
    material = bpy.data.materials.new(name=<span class="hljs-string">'SphereMaterial'</span>)
    material.use_nodes = <span class="hljs-literal">False</span>
    material.diffuse_color = (<span class="hljs-number">1</span>, <span class="hljs-number">1</span>, <span class="hljs-number">1</span>, <span class="hljs-number">1</span>)  <span class="hljs-comment"># Set color to white</span>

    <span class="hljs-keyword">return</span> material

<span class="hljs-comment"># Function to animate the rotating sphere</span>
<span class="hljs-keyword">def</span> <span class="hljs-title function_">animate_rotating_sphere</span>(<span class="hljs-params">mesh, frame_start, frame_end</span>):
    theta, phi = np.mgrid[<span class="hljs-number">0</span>:math.pi:<span class="hljs-number">100j</span>, <span class="hljs-number">0</span>:<span class="hljs-number">2</span> * math.pi:<span class="hljs-number">100j</span>]
    rho = <span class="hljs-number">2.0</span>

    <span class="hljs-keyword">for</span> frame <span class="hljs-keyword">in</span> <span class="hljs-built_in">range</span>(frame_start, frame_end + <span class="hljs-number">1</span>):
        theta_rot = theta + frame / <span class="hljs-number">100</span> * <span class="hljs-number">2</span> * math.pi
        x_rot, y_rot, z_rot = rotating_sphere(theta_rot, phi, rho)

        <span class="hljs-comment"># Update the mesh vertices</span>
        <span class="hljs-keyword">for</span> i, (vx, vy, vz) <span class="hljs-keyword">in</span> <span class="hljs-built_in">enumerate</span>(<span class="hljs-built_in">zip</span>(x_rot.ravel(), y_rot.ravel(), z_rot.ravel())):
            mesh.vertices[i].co = (vx, vy, vz)

        <span class="hljs-comment"># Insert a keyframe for the mesh</span>
        mesh.update()
        mesh.keyframe_insert(data_path=<span class="hljs-string">'vertices'</span>, frame=frame)

<span class="hljs-comment"># Clear existing objects</span>
bpy.ops.<span class="hljs-built_in">object</span>.select_all(action=<span class="hljs-string">'SELECT'</span>)
bpy.ops.<span class="hljs-built_in">object</span>.delete()

<span class="hljs-comment"># Create the rotating sphere</span>
sphere_mesh = create_rotating_sphere()
sphere_material = create_sphere_material()
sphere_mesh.materials.append(sphere_material)

<span class="hljs-comment"># Create and animate the rotating sphere</span>
frame_start = <span class="hljs-number">1</span>
frame_end = <span class="hljs-number">100</span>
