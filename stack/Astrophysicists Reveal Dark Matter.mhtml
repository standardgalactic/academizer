<span class="hljs-keyword">import</span> matplotlib.pyplot <span class="hljs-keyword">as</span> plt
<span class="hljs-keyword">from</span> mpl_toolkits.mplot3d <span class="hljs-keyword">import</span> Axes3D

<span class="hljs-comment"># Create 3D arrays of R, G, and B values from 0-255</span>
r = np.arange(<span class="hljs-number">0</span>, <span class="hljs-number">256</span>, <span class="hljs-number">1</span>)
g = np.arange(<span class="hljs-number">0</span>, <span class="hljs-number">256</span>, <span class="hljs-number">1</span>)
b = np.arange(<span class="hljs-number">0</span>, <span class="hljs-number">256</span>, <span class="hljs-number">1</span>)

<span class="hljs-comment"># Create a meshgrid of R, G, and B values</span>
R, G, B = np.meshgrid(r, g, b)

<span class="hljs-comment"># Combine the R, G, and B values into a 3D array of RGB tuples</span>
rgb = np.stack((R, G, B), axis=<span class="hljs-number">3</span>)

<span class="hljs-comment"># Normalize the RGB values to range from 0 to 1</span>
