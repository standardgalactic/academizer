<span class="hljs-comment"># Plot the RGB color cube</span>
fig = plt.figure(figsize=(<span class="hljs-number">8</span>,<span class="hljs-number">8</span>))
ax = fig.add_subplot(<span class="hljs-number">111</span>, projection=<span class="hljs-string">'3d'</span>)
ax.scatter(R.flatten(), G.flatten(), B.flatten(), s=<span class="hljs-number">1</span>, c=rgb_norm.reshape(-<span class="hljs-number">1</span>, <span class="hljs-number">3</span>))
ax.set_xlabel(<span class="hljs-string">'Red'</span>)
ax.set_ylabel(<span class="hljs-string">'Green'</span>)
ax.set_zlabel(<span class="hljs-string">'Blue'</span>)
plt.show()
