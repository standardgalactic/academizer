blue_material.diffuse_color = (<span class="hljs-number">0</span>,<span class="hljs-number">0</span>,<span class="hljs-number">1</span>)
inner_sphere.data.materials.append(blue_material)

<span class="hljs-comment"># Create a new light object</span>
