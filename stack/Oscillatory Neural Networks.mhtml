    pipeline_data[<span class="hljs-built_in">int</span>(pipeline_length/<span class="hljs-number">2</span>), <span class="hljs-number">0</span>] = mid_delay

    <span class="hljs-comment"># Update the pipeline links based on completeness conditions</span>
    <span class="hljs-keyword">for</span> i <span class="hljs-keyword">in</span> <span class="hljs-built_in">range</span>(pipeline_length - <span class="hljs-number">1</span>):
        <span class="hljs-keyword">if</span> pipeline_data[i, <span class="hljs-number">0</span>] == pipeline_data[i, <span class="hljs-number">1</span>]:
