            ax.text(i + <span class="hljs-number">0.5</span>, <span class="hljs-number">0.5</span>, <span class="hljs-built_in">str</span>(quaternary_value), ha=<span class="hljs-string">'center'</span>, va=<span class="hljs-string">'center'</span>, fontsize=<span class="hljs-number">12</span>)
            ax.arrow(i, <span class="hljs-number">0</span>, <span class="hljs-number">1</span>, <span class="hljs-number">0</span>, width=<span class="hljs-number">0.1</span>, color=<span class="hljs-string">'black'</span>, head_width=<span class="hljs-number">0.3</span>, head_length=<span class="hljs-number">0.2</span>)
    
    plt.show()

<span class="hljs-comment"># Example usage</span>
